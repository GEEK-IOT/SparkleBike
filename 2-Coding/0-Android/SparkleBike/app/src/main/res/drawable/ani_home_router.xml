<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/ic_home_router">

    <target
        android:name="centerPoint"
        android:animation="@animator/anim_blowup" />

    <target
        android:name="waveHidden"
        android:animation="@animator/anim_hiddenwave_ripple_out" />

    <target
        android:name="waveHiddenPath"
        android:animation="@animator/anim_hiddenwave_fade_in" />

    <target
        android:name="waveInner"
        android:animation="@animator/anim_innerwave_ripple_out" />

    <target
        android:name="waveOuter"
        android:animation="@animator/anim_outerwave_ripple_out" />

    <target
        android:name="waveOuterPath"
        android:animation="@animator/anim_outerwave_fade_out" />

</animated-vector>